		<script>
			$(function(){
			  var right_box_height = $(".content_50left").innerHeight() - ($(".sidebar_40right").size() - 1) * 20;
			  right_box_height = right_box_height / $(".sidebar_40right").size() - 40;
			  $(".sidebar_40right").height(right_box_height + 'px');
			  $(".sidebar_40right").each(function() {
				  $(this).find('p')
				  .height(parseInt(right_box_height -20 - $(this).find('h3').innerHeight()) + 'px')
				  .ellipsis();
			  });
			});
